{
  "name": "kaibu",
  "version": "0.1.32",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "check-format": "prettier --check \"{src,tests}/**/**\"",
    "format": "prettier --write \"{src,tests}/**/**\""
  },
  "dependencies": {
    "@mdi/font": "^5.3.45",
    "@turf/turf": "^5.1.6",
    "buefy": "^0.8.0",
    "core-js": "^3.6.5",
    "json-stringify-safe": "^5.0.1",
    "mathjs": "^7.0.1",
    "ol": "^6.5.0",
    "register-service-worker": "^1.7.1",
    "sl-vue-tree": "^1.8.5",
    "sortablejs": "^1.10.2",
    "vega": "^5.7.3",
    "vega-embed": "^6.0.0",
    "vega-lite": "^4.12.2",
    "vue": "^2.6.11",
    "vue-form-generator": "^2.3.4",
    "vue-form-json": "github:oeway/vue-form-json#slot-props",
    "vue-router": "^3.2.0",
    "vue-swatches": "^2.1.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-pwa": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-unit-mocha": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "chai": "^4.1.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^1.19.1",
    "vue-cli-plugin-buefy": "~0.3.7",
    "vue-template-compiler": "^2.6.11",
    "copy-webpack-plugin": "^4.6.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-console": 0,
      "no-debugger": 0
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
